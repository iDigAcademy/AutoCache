{
    "name": "idigacademy/autocache",
    "description": "Automatic model caching for Laravel with MySQL and MongoDB support",
    "type": "library",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "iDigAcademy",
            "email": "idigacademy@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/cache": "^12.0",
        "illuminate/events": "^12.0",
        "illuminate/console": "^12.0",
        "mongodb/laravel-mongodb": "^5.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "IDigAcademy\\AutoCache\\": "src/",
            "IDigAcademy\\AutoCache\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "iDigAcademy\\AutoCache\\Providers\\AutoCacheServiceProvider"
            ]
        }
    }
}
